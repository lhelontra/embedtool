CHROOT_SHAREDDIR="/mnt"
BUILDIMAGE_BOOTFS_TYPE="vfat"
BUILDIMAGE_BOOTFS_TYPE_ID=6
BUILDIMAGE_BOOTFS_START_SECTOR=3072
BUILDIMAGE_BOOTFS_MKFS_ARGS="-F 16 -n boot"
BUILDIMAGE_ROOTFS_TYPE="ext4"
BUILDIMAGE_ROOTFS_MKFS_ARGS="-L rootfs"
BUILDIMAGE_RESERVED_BLOCKS_PERCENT=5
BUILDIMAGE_SYSTEM_EXTRA_SIZE_MB="200"
BUILDIMAGE_BOOTFS_PART_SIZE_MB="128.5"
BUILDIMAGE_AFTERGEN="afterGenImg.sh"
BUILDIMAGE_AFTERCOPYDATA="afterCopyingData.sh"
BOOTFS_MOUNTPOINT="/media/boot"